{
  "name": "@arragro/object-history-web",
  "version": "0.0.8-alpha",
  "description": "Arragro Object History for web projects",
  "main": "dist/arragro-object-history-web.js",
  "scripts": {
    "postinstall-temp": "concurrently \"npm run build-dll\" \"npm run build\"",
    "lint": "tslint -p ./tsconfig.json -c ./tslint.json -e **/node_modules/** -e **/dist/** -e **/types/** -t codeFrame **/*.ts* ",
    "prepush": "npm run lint && npm run build-prod",
    "prepush-lib": "npm run lint && npm run build-lib",
    "publish-prod": "npm run prepush-lib && npm pack",
    "build-dev": "webpack --progress --profile --log-level trace",
    "build-prod": "webpack --run-prod --progress --profile",
    "build-dll": "webpack  --config webpack.dll.config.js --progress --profile",
    "build-lib": "webpack  --run-prod --config ./webpack-lib.production.config.js --progress --profile",
    "build-dll-dev": "npm run build-dll && npm run build-dev",
    "test-tsc": "tsc -p tsconfig.lib.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "run-app": "npm install && npm run build-prod && dotnet run -c Release"
  },
  "files": [
    "dist"
  ],
  "typings": "./dist/types/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.arragro.com/repository/npm-hosted/"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "dotnet",
    "core",
    "cms"
  ],
  "author": "Michael Jewell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arragro/Arragro.ObjectHistory/issues"
  },
