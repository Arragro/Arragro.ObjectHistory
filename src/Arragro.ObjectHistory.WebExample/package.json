{
  "name": "@arragro/object-history",
  "version": "0.0.3-alpha",
  "description": "Arragro Object History for web projects",
  "main": "dist/arragro-object-history.js",
  "scripts": {
    "postinstall-temp": "concurrently \"npm run build-dll\" \"npm run build\"",
    "lint": "eslint \"ReactApp*/**/*.{js,ts,tsx}\" --quiet --fix",
    "prepush": "npm run lint && npm run build-prod",
    "prepush-lib": "npm run lint && npm run build-lib && tsc --project tsconfig.lib.json",
    "publish-prod": "npm run prepush-lib && npm pack",
    "build-dev": "webpack --mode development--progress --profile --log-level trace",
    "build-prod": "webpack --mode production --progress --profile",
    "build-dll": "webpack  --config webpack.dll.config.js --progress --profile",
    "build-lib": "webpack  --mode production --config ./webpack-lib.production.config.js --progress --profile",
    "build-dll-dev": "npm run build-dll && npm run build-dev",
    "test-tsc": "tsc -p tsconfig.lib.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "run-app": "npm install && npm run build-prod && dotnet run -c Release"
  },
  "files": [
    "dist"
  ],
  "typings": "./dist/types/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.arragro.com/repository/npm-hosted/"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "dotnet",
    "core",
    "cms"
  ],
  "author": "Michael Jewell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arragro/Arragro.ObjectHistory/issues"
  },
  "dependencies": {
    "@date-io/dayjs": "^2.10.8",
    "bootstrap": "^3.4.1",
    "dayjs": "^1.10.4",
    "formik": "^2.2.6",
    "history": "^5.0.0",
    "js-cookie": "^2.2.1",
    "jsondiffpatch": "^0.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.13.5",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typesafe-actions": "^5.1.0",
    "yup": "^0.32.9"
  },
  "browserslist ": [
    "last 2 versions"
  ],
  "devDependencies": {
    "@babel/cli": "^7.13.10",
