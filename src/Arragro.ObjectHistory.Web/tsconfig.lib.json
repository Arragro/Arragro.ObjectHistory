{
    "compilerOptions": {
        "sourceMap": true,
        "keyofStringsOnly": true,
        "target": "es5",
        "jsx": "react",
        "module": "es6",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "declaration": true,
        "declarationDir": "./dist/types",
        "removeComments": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "strict": true,
        "outDir": "./dist/",
        "lib": ["es6", "es7", "dom"]
    },
    "compileOnSave": false,
    "include": [
        "./ReactAppLibrary/**/*.ts*",
        "./types"
    ],
    "exclude": ["dist", "build", "node_modules", "App"]
}
